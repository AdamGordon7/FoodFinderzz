<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.restaurantv2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Restaurantv2"
        android:usesCleartextTraffic="true">
        <activity android:name=".ChangeStatus"></activity>
        <activity android:name=".CreateOrder" />
        <activity android:name=".CustomerOrderInfo" />
        <activity android:name=".StaffMainView" />
        <activity android:name=".CustomerMainView" />
        <activity android:name=".StaffSignUp" />
        <activity android:name=".StaffLoginSignUp" />
        <activity android:name=".CustomerSignUp" />
        <activity android:name=".CustomerLoginSignup" />
        <activity android:name=".FrontPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>